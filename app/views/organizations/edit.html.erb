<% html_title l(:label_administration), "#{l(:label_edit)} #{Organization.model_name.human} #{h @organization.name}" %>

<% local_assigns[:additional_breadcrumb] =  @organization %>
<%= breadcrumb_toolbar @organization %>

<%= labelled_tabular_form_for @organization do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <%= styled_button_tag l(:button_save), class: '-highlight -with-icon icon-checkmark' %>
<% end %>

<%= styled_form_tag(organization_path(@organization), method: :delete) do %>
  <%= styled_submit_tag l(:button_delete) %>
<% end %>
